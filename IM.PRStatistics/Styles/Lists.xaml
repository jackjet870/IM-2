<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:IM.PRStatistics.Styles">

    <DataTemplate x:Key="ListsLeadSources">
        <TextBlock Text="{Binding lsN}" Foreground="Black" Width="100" ToolTip="{Binding lsN}"/>
    </DataTemplate>
    <DataTemplate x:Key="ListsCountries">
        <TextBlock Text="{Binding coN}" Foreground="Black" Width="100" ToolTip="{Binding coN}"/>
    </DataTemplate>

    <DataTemplate x:Key="ListsAgencies">
        <TextBlock Text="{Binding agN}" Foreground="Black" Width="100" ToolTip="{Binding agN}"/>
    </DataTemplate>

    <DataTemplate x:Key="ListsSalesRooms">
        <TextBlock Text="{Binding srN}" Foreground="Black" Width="100" ToolTip="{Binding srN}"/>
    </DataTemplate>
    <DataTemplate x:Key="ListsMarkets">
        <TextBlock Text="{Binding mkN}" Foreground="Black" Width="100" ToolTip="{Binding mkN}"/>
    </DataTemplate>
</ResourceDictionary>